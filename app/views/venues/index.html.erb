<h1>Listing all Venues</h1>
<table>
	<tr>
		<th colspan="5" style="text-align:right"><%= link_to 'Add New Venue', new_venue_path %></th>
	</tr>
	<tr>
		<th>Name</th>
		<th>Postcode</th>
		<th>Styles</th>
		<th>Meeting Rooms</th>
		<th>Action</th>
	</tr>
	<%- for venue in @venues %>
	<tr>
		<td><%= link_to venue.name, venue_path(venue) %></td>
		<td><%= venue.postcode %></td>
		<td>
		<ul>
			<%- for style in venue.style -%>
			<li><%= style.style %></li>
			<%- end -%>
		</ul>
		</td>
		<td>
		<ul>
			<%- for room in venue.meeting_room -%>
			<li><%= room.name %></li>
			<%- end -%>
		</ul>
		</td>
		<td><%= link_to 'Edit', edit_venue_path(venue) %></td>
	</tr>
	<%- end %>
</table>